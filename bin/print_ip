#!/usr/bin/env python
import sys
import socket

def get_self_ip():
    s = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)
    dummy_remote = '10.10.10.10'
    try:
        s.connect((dummy_remote, 9))
        client = s.getsockname()[0]
    except socket.error:
        client = "Unknown IP"
    finally:
        del s
    return client

print get_self_ip()
